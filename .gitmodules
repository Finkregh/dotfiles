#   dont forget to:
#       cd bin/gitflow.git; git submodule init ; git submodule update
[submodule ".bash_completion.d/git-flow-completion"]
	path = .bash_completion.d/git-flow-completion
	url = git://github.com/bobthecow/git-flow-completion.git
[submodule ".global_ignore"]
	path = .global_ignore
	url = git@github.com:Finkregh/gitignore.git
[submodule "bin"]
	path = bin
	url = git@github.com:Finkregh/dotfiles-bin.git
[submodule ".vim"]
	path = .vim
	url = git@github.com:Finkregh/dotfiles-vim.git
